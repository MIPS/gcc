# 2 multilibs:
#  ./ -march=armv7 -mthumb -mfloat-abi=softfp -mfpu=vfpv3-d16
#  v7a/	 -march=armv7-a -marm -mfloat-abi=softfp -mfpu=vfpv3-d16

MULTILIB_OPTIONS = \
	march=armv7-a \
	marm

MULTILIB_DIRNAMES = \
	v7-a \
	arm \

MULTILIB_EXCEPTIONS = \
	marm \
	march=armv7-a \

MULTILIB_OSDIRNAMES = \
	march.armv7-a/marm=!v7a \
		        
MULITLIB_MATCHES =

MULTILIB_ALIASES =
