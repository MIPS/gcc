# The default build is mips32r6, hard-float big-endian.

MULTILIB_OPTIONS = mips64r6/mips32r2/mips64r2 mmicromips EL mfp64 mnan=2008
MULTILIB_DIRNAMES = mips64r6 mips32r2 mips64r2 micromips el fp64 nan2008
MULTILIB_MATCHES = EL=mel EB=meb mips32r2=mips32r5 mips64r2=mips64r3 mips64r2=mips64r5

MULTILIB_EXCEPTIONS += *mips64r6/mmicromips*
MULTILIB_EXCEPTIONS += mmicromips*

MULTILIB_EXCEPTIONS += *mips32r6/*mfp64*
MULTILIB_EXCEPTIONS += *mips64/*mfp64*
