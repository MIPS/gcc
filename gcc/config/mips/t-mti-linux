# The default build is mips32r2, hard-float big-endian.  Add mips32,
# soft-float, and little-endian variations.

MULTILIB_OPTIONS = mips32/mips64/mips64r2 mips16/mmicromips mabi=64 EL msoft-float/mfp64 mnan=2008
MULTILIB_DIRNAMES = mips32 mips64 mips64r2 mips16 micromips 64 el sof fp64 nan2008
MULTILIB_MATCHES = EL=mel EB=meb mips32r2=mips32r3 mips32r2=mips32r5 mips64r2=mips64r3 mips64r2=mips64r5

# The 64 bit ABI is not supported on the mips32 architecture.
MULTILIB_EXCEPTIONS += *mips32*/*mabi=64*

# The 64 bit ABI is not supported on the mips32r2 architecture.
# Because mips32r2 is the default we can't use that flag to trigger
# the exception so we check for mabi=64 with no specific mips
# architecture flag instead.
MULTILIB_EXCEPTIONS += mabi=64*

# We do not want to build mips16 versions of mips64* architectures.
MULTILIB_EXCEPTIONS += *mips64*/*mips16*
MULTILIB_EXCEPTIONS += *mips16/mabi=64*

# We only want micromips for mips32r2 architecture.
MULTILIB_EXCEPTIONS += *mips32/mmicromips*
MULTILIB_EXCEPTIONS += *mips64*/mmicromips*
MULTILIB_EXCEPTIONS += *mmicromips/mabi=64*

# We do not want nan2008 libraries for soft-float.
MULTILIB_EXCEPTIONS += *msoft-float*/*mnan=2008*

# -mfp64 libraries are only built for mips32r2 and not in mips16 mode.
MULTILIB_EXCEPTIONS += *mips32/*mfp64*
MULTILIB_EXCEPTIONS += *mips64*/*mfp64*
MULTILIB_EXCEPTIONS += *mips16*/*mfp64*

# MULTILIB_EXCEPTIONS += .;
MULTILIB_EXCEPTIONS += mips32
MULTILIB_EXCEPTIONS += mips64
MULTILIB_EXCEPTIONS += mips64r2
MULTILIB_EXCEPTIONS += mips16
MULTILIB_EXCEPTIONS += mmicromips
MULTILIB_EXCEPTIONS += EL
MULTILIB_EXCEPTIONS += mfp64
#MULTILIB_EXCEPTIONS += mfp64/mnan=2008
MULTILIB_EXCEPTIONS += msoft-float
MULTILIB_EXCEPTIONS += mnan=2008
MULTILIB_EXCEPTIONS += EL/msoft-float
MULTILIB_EXCEPTIONS += EL/mfp64
#MULTILIB_EXCEPTIONS += EL/mnan=2008
#MULTILIB_EXCEPTIONS += EL/mfp64/mnan=2008
MULTILIB_EXCEPTIONS += mips16/EL
MULTILIB_EXCEPTIONS += mips16/msoft-float
MULTILIB_EXCEPTIONS += mips16/mnan=2008
MULTILIB_EXCEPTIONS += mips16/EL/msoft-float
MULTILIB_EXCEPTIONS += mips16/EL/mnan=2008
MULTILIB_EXCEPTIONS += mmicromips/EL
MULTILIB_EXCEPTIONS += mmicromips/msoft-float
MULTILIB_EXCEPTIONS += mmicromips/mnan=2008
MULTILIB_EXCEPTIONS += mmicromips/mfp64
MULTILIB_EXCEPTIONS += mmicromips/mfp64/mnan=2008
MULTILIB_EXCEPTIONS += mmicromips/EL/mfp64
MULTILIB_EXCEPTIONS += mmicromips/EL/msoft-float
MULTILIB_EXCEPTIONS += mmicromips/EL/mnan=2008
MULTILIB_EXCEPTIONS += mmicromips/EL/mfp64/mnan=2008
MULTILIB_EXCEPTIONS += mips32/mips16
MULTILIB_EXCEPTIONS += mips32/EL
MULTILIB_EXCEPTIONS += mips32/msoft-float
MULTILIB_EXCEPTIONS += mips32/mnan=2008
MULTILIB_EXCEPTIONS += mips32/EL/msoft-float
MULTILIB_EXCEPTIONS += mips32/EL/mnan=2008
MULTILIB_EXCEPTIONS += mips32/mips16/EL
MULTILIB_EXCEPTIONS += mips32/mips16/msoft-float
MULTILIB_EXCEPTIONS += mips32/mips16/mnan=2008
MULTILIB_EXCEPTIONS += mips32/mips16/EL/msoft-float
MULTILIB_EXCEPTIONS += mips32/mips16/EL/mnan=2008
MULTILIB_EXCEPTIONS += mips64/mabi=64
MULTILIB_EXCEPTIONS += mips64/EL
MULTILIB_EXCEPTIONS += mips64/msoft-float
MULTILIB_EXCEPTIONS += mips64/mnan=2008
MULTILIB_EXCEPTIONS += mips64/EL/msoft-float
MULTILIB_EXCEPTIONS += mips64/EL/mnan=2008
MULTILIB_EXCEPTIONS += mips64/mabi=64/EL
MULTILIB_EXCEPTIONS += mips64/mabi=64/msoft-float
MULTILIB_EXCEPTIONS += mips64/mabi=64/mnan=2008
MULTILIB_EXCEPTIONS += mips64/mabi=64/EL/msoft-float
MULTILIB_EXCEPTIONS += mips64/mabi=64/EL/mnan=2008
MULTILIB_EXCEPTIONS += mips64r2/mabi=64
MULTILIB_EXCEPTIONS += mips64r2/EL
MULTILIB_EXCEPTIONS += mips64r2/msoft-float
MULTILIB_EXCEPTIONS += mips64r2/mnan=2008
MULTILIB_EXCEPTIONS += mips64r2/EL/msoft-float
MULTILIB_EXCEPTIONS += mips64r2/EL/mnan=2008
MULTILIB_EXCEPTIONS += mips64r2/mabi=64/EL
MULTILIB_EXCEPTIONS += mips64r2/mabi=64/msoft-float
MULTILIB_EXCEPTIONS += mips64r2/mabi=64/mnan=2008
MULTILIB_EXCEPTIONS += mips64r2/mabi=64/EL/msoft-float
#MULTILIB_EXCEPTIONS += mips64r2/mabi=64/EL/mnan=2008
