# The default build is mips32r2, hard-float big-endian.  Add mips32,
# soft-float, and little-endian variations.

MULTILIB_OPTIONS = muclibc mips64r2 mmicromips mabi=32/mabi=n32/mabi=64 EL mnan=2008
MULTILIB_DIRNAMES = uclibc mips64r2 micromips 32 n32 64 el nan2008
MULTILIB_MATCHES = EL=mel EB=meb mips32r2=mips32r3 mips32r2=mips32r5 mips64r2=mips64r3 mips64r2=mips64r5

MULTILIB_EXCLUSIONS = !muclibc/!mips64r2/!mmicromips/!mabi=32/!mabi=n32/!mabi=64/!EL/!mnan=2008

MULTILIB_REQUIRED   = mabi=32
MULTILIB_OSDIRNAMES = mabi.32=!mips-r2-hard/lib
MULTILIB_REUSE      = mabi.32=mips64r2/mabi.32
MULTILIB_REQUIRED   += mips64r2/mabi=n32
MULTILIB_OSDIRNAMES += mips64r2/mabi.n32=!mips-r2-hard/lib32
MULTILIB_REQUIRED   += mips64r2/mabi=64
MULTILIB_OSDIRNAMES += mips64r2/mabi.64=!mips-r2-hard/lib64

MULTILIB_REQUIRED   += mabi=32/mnan=2008
MULTILIB_OSDIRNAMES += mabi.32/mnan.2008=!mips-r2-hard-nan2008/lib
MULTILIB_REUSE      += mabi.32/mnan.2008=mips64r2/mabi.32/mnan.2008
#MULTILIB_REQUIRED   += mips64r2/mabi=n32/mnan=2008
#MULTILIB_OSDIRNAMES += mips64r2/mabi.n32/mnan.2008=!mips-r2-hard-nan2008/lib32
#MULTILIB_REQUIRED   += mips64r2/mabi=64/mnan=2008
#MULTILIB_OSDIRNAMES += mips64r2/mabi.64/mnan.2008=!mips-r2-hard-nan2008/lib64

