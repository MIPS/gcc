# Uclibc multilibs
MULTILIB_REQUIRED   += muclibc/mabi=32
MULTILIB_OSDIRNAMES += muclibc/mabi.32=!mips-r2-hard-uclibc/lib
MULTILIB_REUSE      += muclibc/mabi.32=muclibc/mips64r2/mabi.32
MULTILIB_REQUIRED   += muclibc/mabi=32/mnan=2008
MULTILIB_OSDIRNAMES += muclibc/mabi.32/mnan.2008=!mips-r2-hard-nan2008-uclibc/lib
MULTILIB_REUSE      += muclibc/mabi.32/mnan.2008=muclibc/mips64r2/mabi.32/mnan.2008
MULTILIB_REQUIRED   += muclibc/mabi=32/EL
MULTILIB_OSDIRNAMES += muclibc/mabi.32/EL=!mipsel-r2-hard-uclibc/lib
MULTILIB_REUSE      += muclibc/mabi.32/EL=muclibc/mips64r2/mabi.32/EL
MULTILIB_REQUIRED   += muclibc/mabi=32/EL/mnan=2008
MULTILIB_OSDIRNAMES += muclibc/mabi.32/EL/mnan.2008=!mipsel-r2-hard-nan2008-uclibc/lib
MULTILIB_REUSE      += muclibc/mabi.32/EL/mnan.2008=muclibc/mips64r2/mabi.32/EL/mnan.2008
