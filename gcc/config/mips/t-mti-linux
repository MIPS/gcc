MULTILIB_REQUIRED   += mabi=32/EL
MULTILIB_OSDIRNAMES += mabi.32/EL=!mipsel-r2-hard/lib
MULTILIB_REUSE      += mabi.32/EL=mips64r2/mabi.32/EL
