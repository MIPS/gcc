.NOTPARALLEL:
$(HOOKOBJS): mf-hooks.c mf-runtime.h mf-impl.h
	set -e; \
	for i in $(HOOKOBJS); do \
	     hook=`basename $$i -hook.lo`; \
	     $(LTCOMPILE) -DWRAP_$$hook -c $(srcdir)/mf-hooks.c -o $$i; \
	done

